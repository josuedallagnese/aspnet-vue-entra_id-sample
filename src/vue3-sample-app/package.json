  },
  "dependencies": {
    "@azure/msal-browser": "^2.30.0",
    "element-plus": "^2.2.19",
    "vue": "^3.2.41",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.1.2",
    "typescript": "^4.8.4",
    "vite": "^3.1.8",
    "vue-tsc": "^1.0.8"
  }
}
